package com.kavipriya;
import java.util.Scanner;

public class Main {

    public static void main( String[] args) {
        int a,b,c;
        Scanner d = new Scanner(System.in);
        System.out.print("enter a number:");
        c= d.nextInt();
                for(a=1;a<=c;a++){
                    for(b=a;b<=c;b++){
                        System.out.print(a+" ");
                   }
                    System.out.println();

    }


}
}
